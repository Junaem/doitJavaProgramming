package sorting_ex;

public class QuickSort implements Sort{

	@Override
	public void ascedning(int[] a) {
		System.out.println("QuickSort ascedning");
		
	}

	@Override
	public void descending(int[] d) {
		System.out.println("QuickSort descending");
		
	}
	@Override
	void description() {
		super.desciption();
	}
	
}
