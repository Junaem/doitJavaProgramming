package arraylist;
import java.util.ArrayList;
public class DogTestArrayList {

	public static void main(String[] args) {
		ArrayList<Dog> dogs =new ArrayList<Dog>();
		
		dogs.add(new Dog("멍멍이", "푸들"));
		dogs.add(new Dog("댕댕이","코카스패니얼"));
		dogs.add(new Dog("츄츄", "시츄"));
		dogs.add(new Dog("세키","시바견"));
		dogs.add(new Dog("코카","보더콜리"));
		
		for(Dog d:dogs) {
			d.showDogInfo();
		}

	}

}
