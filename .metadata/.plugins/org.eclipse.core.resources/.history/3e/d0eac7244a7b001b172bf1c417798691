package arraylist;

public class DogTest {

	public static void main(String[] args) {
		Dog[] dogs = new Dog[5];
		char ch ='가';
		
		for(Dog a:dogs) {
			a.setName(ch+"돌이");
		}
		dogs[0].setType("말티즈");
		dogs[1].setType("달마시안");
		dogs[2].setType("치와와");
		dogs[3].setType("캐버리얼 킹찰스 스패니얼");
		dogs[4].setType("샴");

	}

}
